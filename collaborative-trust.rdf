<rdf:Description rdf:about="http://purl.org/voc/collaborative-trust/">
	<rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
	<voaf:classNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</voaf:classNumber>
	<voaf:propertyNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</voaf:propertyNumber>
	<voaf:specializes rdf:resource="http://www.w3.org/ns/prov#"/>
	<vann:preferredNamespacePrefix>trust</vann:preferredNamespacePrefix>
	<vann:prefferedNamespaceUri rdf:resource="http://purl.org/vocab/collaborative-trust/"/>
	<dcterms:creator xml:lang="en">Adam M. Leadbetter</dcterms:creator>
	<dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-12-12</dcterms:created>
	<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-01-06</dcterms:modified>
	<owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</owl:versionInfo>
